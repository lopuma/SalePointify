{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "space-before-function-paren": "off",
        "react/prop-types": "off",
        "react/no-unescaped-entities": "off",
        "@next/next/no-page-custom-font": "off",
        "indent": "off",
        "no-tabs": "off"
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}